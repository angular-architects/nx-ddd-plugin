{
  "root": "libs/ddd",
  "sourceRoot": "libs/ddd/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ddd/**/*.ts",
          "libs/ddd/**/*.spec.ts",
          "libs/ddd/**/*.spec.tsx",
          "libs/ddd/**/*.spec.js",
          "libs/ddd/**/*.spec.jsx",
          "libs/ddd/**/*.d.ts"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/ddd/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/ddd"]
    },
    "build": {
      "executor": "@nrwl/node:package",
      "options": {
        "outputPath": "dist/libs/ddd",
        "tsConfig": "libs/ddd/tsconfig.lib.json",
        "packageJson": "libs/ddd/package.json",
        "main": "libs/ddd/src/index.ts",
        "assets": [
          {
            "input": "./libs/ddd",
            "glob": "**/*.*",
            "output": "./libs/ddd"
          },
          "libs/ddd/*.md",
          {
            "input": "./libs/ddd/src",
            "glob": "**/*.*",
            "output": "./libs/ddd/src"
          },
          {
            "input": "./libs/ddd",
            "glob": "collection.json",
            "output": "."
          },
          {
            "input": "./libs/ddd",
            "glob": "builders.json",
            "output": "."
          }
        ]
      }
    }
  },
  "tags": []
}
