{
  "name": "angular-architects",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "yarn dlx nx affected",
    "affected:apps": "yarn dlx nx affected:apps",
    "affected:build": "yarn dlx nx affected:build",
    "affected:dep-graph": "yarn dlx nx affected:dep-graph",
    "affected:e2e": "yarn dlx nx affected:e2e",
    "affected:libs": "yarn dlx nx affected:libs",
    "affected:lint": "yarn dlx nx affected:lint",
    "affected:test": "yarn dlx nx affected:test",
    "build": "yarn dlx nx build ddd",
    "build:schema": "node packages/ddd/src/generators/json-schema-to-ts.js",
    "commit": "git-cz",
    "dep-graph": "yarn dlx nx dep-graph",
    "e2e:ddd:": "yarn dlx nx run ddd-e2e:e2e",
    "format": "yarn dlx nx format:write",
    "format:check": "yarn dlx nx format:check",
    "format:write": "yarn dlx nx format:write",
    "help": "yarn dlx nx help",
    "lint": "yarn dlx nx workspace-lint && nx lint",
    "nx": "yarn dlx nx",
    "pack": "npm pack ./dist/packages/ddd",
    "start": "yarn dlx nx serve",
    "test": "yarn dlx nx test",
    "update": "yarn dlx nx migrate latest",
    "workspace-generator": "yarn dlx nx workspace-generator"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@schematics/angular": "^12.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nrwl/angular": "13.1.4",
    "@nrwl/cli": "13.1.4",
    "@nrwl/devkit": "13.1.4",
    "@nrwl/eslint-plugin-nx": "13.1.4",
    "@nrwl/jest": "13.1.4",
    "@nrwl/linter": "13.1.4",
    "@nrwl/node": "13.1.4",
    "@nrwl/nx-plugin": "13.1.4",
    "@nrwl/tao": "13.1.4",
    "@nrwl/workspace": "13.1.4",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@yarnpkg/plugin-dlx": "^3.1.1",
    "commitizen": "^4.2.2",
    "cz-customizable": "^6.3.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "json-schema-to-typescript": "^8.1.0",
    "nx": "13.1.4",
    "prettier": "2.5.0",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
  "packageManager": "yarn@3.2.0-rc.6"
}
