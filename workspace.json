{
  "version": 1,
  "projects": {
    "ddd": {
      "root": "libs/ddd",
      "sourceRoot": "libs/ddd/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ddd/**/*.ts",
              "libs/ddd/**/*.spec.ts",
              "libs/ddd/**/*.spec.tsx",
              "libs/ddd/**/*.spec.js",
              "libs/ddd/**/*.spec.jsx",
              "libs/ddd/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ddd/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/ddd"]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/ddd",
            "tsConfig": "libs/ddd/tsconfig.lib.json",
            "packageJson": "libs/ddd/package.json",
            "main": "libs/ddd/src/index.ts",
            "assets": [
              {
                "input": "./libs/ddd",
                "glob": "**/*.*",
                "output": "./libs/ddd"
              },
              "libs/ddd/*.md",
              {
                "input": "./libs/ddd/src",
                "glob": "**/*.*",
                "output": "./libs/ddd/src"
              },
              {
                "input": "./libs/ddd",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/ddd",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      },
      "tags": []
    },
    "ddd-e2e": {
      "projectType": "application",
      "root": "apps\\ddd-e2e",
      "sourceRoot": "apps\\ddd-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "ddd:build",
            "npmPackageName": "@angular-architects/ddd",
            "pluginOutputPath": "dist/libs/ddd",
            "jestConfig": "apps/ddd-e2e/jest.config.js"
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["ddd"]
    }
  }
}
